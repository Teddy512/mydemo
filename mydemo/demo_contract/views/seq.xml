<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1"><!-- Sequences for contract -->
        <record id="seq_demo_contract" model="ir.sequence">
            <field name="name">立项</field>
            <field name="code">demo.contract.lx</field>
            <field name="prefix">FD</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
        </record>
    </data>

    <data>
        <record id="seq_settle_account" model="ir.sequence">
            <field name="name">预算单</field>
            <field name="code">settle.account</field>
            <field name="prefix">YS</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
        </record>
    </data>

    <data noupdate="1"><!-- Sequences for 合同 -->
        <record id="seq_sigining_contract" model="ir.sequence">
            <field name="name">合同单</field>
            <field name="code">sigining.contract</field>
            <field name="prefix">FSD</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
        </record>
    </data>





</odoo>